        width: 80%;
        height: 80%;
        margin: 10% auto;
        display: block;
        fill: white;
      }

      .heading {
        font-size: 34px;
        font-weight: 300;
        line-height: 1.67;
        margin: 0;
      }

      .content {
        color: #8d8d8d;
        font-size: 20px;
        line-height: 1.5;
        width: 258px;
        text-align: center;
        font-family: "Georgia","Times","Times New Roman",serif;
        font-style: italic;
      }

      a svg {
        width: 24px;
        height: 24px;
      }

      .social-links {
        display: flex;
        justify-content: space-between;
        width: 120px;
      }

      @media( min-width: 769px ) {
        .container {
          max-width: 1200px;
          height: 100%;
          padding-top: 0;
          padding-left: 100px;
          align-items: flex-start;
        }

        .site-logo {
          margin-bottom: 20px;
          width: 100px;
          height: 100px;
        }

        .content {
          width: 50%;
          text-align: left;
          font-family: "Georgia","Times","Times New Roman",serif;
          font-style: italic;
        }

        .heading {
          font-size: 50px;
          line-height: 1.7;
        }

        .social-links {
          margin-top: 25px;
        }

        p {
          margin: 0;
        }
      }

      @media( min-width: 1400px ) {
        .container {
          padding-right: 0;
        }
      }
    </style>
