    <script type="text/javascript">
        if ($('.start-page-content-form-top').length > 0) {
            var media = window.matchMedia("(min-width: 768px)");
            var headerelement = $('.start-page-content-form-top > .root > .cmp-responsivegrid > div > .header > header > div > .responsivegrid');
            var headerTopElement = $('.start-page-content-form-top > .root > .cmp-responsivegrid > div > .header');
            var headerStyle = { 'background': 'transparent', 'position': 'fixed', 'z-index': 201, 'left': 0, 'top': '37px' };
            var stickyClass = 'sticky';
            var $el, $el_top, $el_height;
            var mediaTopContent = window.matchMedia("(max-width: 992px)");
            headerelement.each(function () {
                $el = $(this);
                $el_top = $el.offset().top;
                $el_height = $el.height();

                $(window).scroll(function () {
                    var $window_top = $(window).scrollTop() - 0;
                    if ($window_top > $el_top) {
                        $el.addClass(stickyClass);
                    } else {
                        $el.removeClass(stickyClass);
                    }
                    if (media.matches) {
                        $('body').css('padding-top', ($window_top > $el_top) ? $el_height : 0);
                    }
                });
                if (media.matches) {
                    $el.css({ "margin-bottom": -$el_height + "px", "z-index": 201 });
                }
                if (mediaTopContent.matches) {
                    headerTopElement.css(headerStyle);
                };
            });

            $(window).on('resize', function () {
                if ($(window).width() < 992) {
                    headerTopElement.css(headerStyle);
                    headerelement.css({ "margin-bottom": "" });
                } else {
                    headerTopElement.removeAttr('style');
                    headerelement.each(function () {
                        $el = $(this);
                        $el_height = $el.height();
                        $el.css({ "margin-bottom": -$el_height + "px", "z-index": 201 });
                    });
                }
            });
        }
    </script>
