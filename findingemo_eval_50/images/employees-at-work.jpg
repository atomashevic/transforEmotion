<script>
  // Define globally
  function showCityLocations(cityName) {
    const locationsList = document.getElementById("locations-list");
    const activeTab = document.querySelector(".service-tab.active")?.textContent?.trim();
    const rawData = document.getElementById("megaMenuData").textContent;
    const data = JSON.parse(rawData);

    const cities = data[activeTab]?.cities || [];
    const cityData = cities.find(c => c.name === cityName);
    if (!cityData) return;

    locationsList.innerHTML = "";
    cityData.locations.forEach(loc => {
      const a = document.createElement("a");
      a.className = "location-item";
      a.href = loc.href;
      a.textContent = loc.label;
      if (loc.target) a.target = "_blank";
      if (loc.rel) a.rel = loc.rel;
      locationsList.appendChild(a);
    });
  }

  document.addEventListener("DOMContentLoaded", function () {
    const tabs = document.querySelectorAll(".service-tab");
    const citiesList = document.querySelector(".cities-list");
    const locationsList = document.getElementById("locations-list");

    const rawData = document.getElementById("megaMenuData").textContent;
    const data = JSON.parse(rawData);

    // On service tab hover â€” show cities
    tabs.forEach(tab => {
      tab.addEventListener("mouseenter", function () {
        tabs.forEach(t => t.classList.remove("active"));
        tab.classList.add("active");

        const title = tab.textContent.trim();
        const cities = data[title]?.cities || [];

        citiesList.innerHTML = "";
        locationsList.innerHTML = "";

        cities.forEach(city => {
          const div = document.createElement("div");
          div.className = "city-item";

          const link = document.createElement("a");
          link.href = city.href || "#";
          link.textContent = city.name;
          if (city.target) link.target = "_blank";
          if (city.rel) link.rel = city.rel;
          div.appendChild(link);

          div.addEventListener("mouseenter", () => showCityLocations(city.name));

          div.addEventListener("click", function (e) {
            if (window.innerWidth < 1090 && e.target.tagName !== "A") {
              document.querySelectorAll(".city-item").forEach(item => item.classList.remove("active"));
              this.classList.add("active");
            }
          });

          citiesList.appendChild(div);
        });
      });
    });

    // Trigger first tab hover initially
    if (tabs.length > 0) {
      tabs[0].dispatchEvent(new Event('mouseenter'));
    }

    // Clean up mobile active class on resize
    window.addEventListener("resize", function () {
      if (window.innerWidth >= 1090) {
        document.querySelectorAll(".city-item").forEach(item => item.classList.remove("active"));
      }
    });

    // Handle tab active/reactive class
    tabs.forEach(tab => {
      tab.addEventListener("mouseenter", () => {
        tabs.forEach(t => t.classList.remove("active", "reactive"));
        tab.classList.add("active");
      });
      tab.addEventListener("click", () => {
        tabs.forEach(t => t.classList.remove("reactive"));
        tab.classList.add("reactive");
      });
    });
  });


  document.addEventListener("DOMContentLoaded", function () {
    const cityItems = document.querySelectorAll(".city-item");

    function handleCityClick(e) {
      if (window.innerWidth < 1090) {
        // Remove 'active' from all city items
        cityItems.forEach(item => item.classList.remove("active"));
        // Add 'active' to the clicked item
        this.classList.add("active");
      }
    }

    cityItems.forEach(item => {
      item.addEventListener("click", handleCityClick);
    });

    // Optional: Remove 'active' when screen resizes above 1090px
    window.addEventListener("resize", function () {
      if (window.innerWidth >= 1090) {
        cityItems.forEach(item => item.classList.remove("active"));
      }
    });
  });


</script>



